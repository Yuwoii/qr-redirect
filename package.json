{
  "name": "qr-redirect",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/setup-local.js && next dev",
    "build": "node scripts/db-migrate.js && prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "migrate": "prisma migrate deploy",
    "db:migrate": "node scripts/db-migrate.js",
    "db:migrate:dev": "node scripts/db-migrate.js --env=dev",
    "db:migrate:prod": "node scripts/db-migrate.js --env=prod",
    "db:check-vercel": "node scripts/vercel-check.js",
    "vercel-build": "chmod +x scripts/*.js && npm run db:check-vercel && npm run db:migrate --env=vercel && next build || (echo 'Trying fallback build process...' && node scripts/db-migrate.js --env=vercel && prisma generate && next build)",
    "db:validate": "node scripts/validate-schema.js",
    "prisma:seed": "tsx prisma/seed.ts",
    "local:setup": "node scripts/setup-local.js",
    "dev:clean": "npm run local:setup && npm run dev"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.3",
    "@prisma/client": "^6.5.0",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@types/qrcode": "^1.5.5",
    "@types/react-color": "^3.0.13",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.483.0",
    "next": "^15.2.3",
    "next-auth": "^5.0.0-beta.25",
    "prisma": "^6.5.0",
    "qrcode": "^1.5.3",
    "qrcode-generator": "^1.4.4",
    "react": "^19.0.0",
    "react-color": "^2.19.3",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "shadcn-ui": "^0.9.5",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@auth/core": "^0.38.0",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/forms": "^0.5.10",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.17.26",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.2.3",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  }
}
