  "installCommand": "npm install",
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "DATABASE_PROVIDER": "postgresql"
    }
  },
  "functions": {
    "api/**/*": {
      "memory": 1024
    }
  }
} 